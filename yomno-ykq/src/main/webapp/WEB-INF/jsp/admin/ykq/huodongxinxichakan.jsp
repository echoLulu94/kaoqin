<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="/common/taglibs.jsp"%>

<div class="table-box bg-white">
	<div class="left-name">
		<ul>
			<li>项目名称</li>
			<li>项目团队</li>
			<li>开始考勤时间</li>
			<li>结束考勤时间</li>
			<li>考勤员</li>
			<li>项目地点</li>
			<li>项目简介</li>
			<li>是否发送通知</li>
			<li>通知内容</li>
		</ul>
	</div>
	<div class="right-form">
		<form role="form" id="myForm">
			<div class="input-div">
				<input type="text" class="form-control" name="projectName" id="projectName">
			</div>
			<div class="input-div">
				<input type="hidden" name="projectTeam" id="projectTeam">
				<a href="huodongcanjiarenyuan.html" class="edit-a">编辑</a>
			</div>
			<div class="input-div">
				<input type="text" class="form-control" name="startAttendance" id="startAttendance">
			</div>
			<div class="input-div">
				<input type="text" class="form-control" name="endAttendance" id="endAttendance">
			</div>
			<div class="input-div">
				<input type="text" class="form-control" name="attendanceMember" id="attendanceMember">
			</div>
			<div class="input-div">
				<input type="text" class="form-control" name="projectSite" id="projectSite">
			</div>
			<div class="input-div">
				<input type="text" class="form-control" name="projectIntroduction" id="projectIntroduction">
			</div>
			<div class="input-div">
				<label class="radio-inline">
					<input type="radio" name="sendNotification" id="sendNotification1" value="1" checked> 发送通知
				</label>
				<label class="radio-inline">
					<input type="radio" name="sendNotification" id="sendNotification2"  value="2"> 不发送通知
				</label>
			</div>
			<div class="input-div">
				<input type="text" class="form-control" name="informContent" id="informContent">
			</div>
			<div class="two-btn">
				<button class="btn btn-specail">保存</button>
				<button class="btn btn-specail">取消</button>
			</div>
		</form>
	</div>
</div>

